        "conbody": {
            "p": [
                {
                    "b": {
                        "ph": {
                            "@keyref": "shepardsfirst"
                        },
                        "#tail": "pane to the right of the document lets you"
                    },
                    "i": {
                        "#tail": "\u00ae the document.",
                        "#text": "Shepardize"
                    },
                    "uicontrol": {
                        "#tail": "provides more information about the document and access to the",
                        "#text": "About This Document"
                    },
                    "ph": {
                        "@keyref": "litnamefirst",
                        "#tail": "so you can track the legal issues addressed in the case law document."
                    },
                    "#text": "When you select a document from your results, the full text of the document is displayed. The"
                },
                {
                    "uicontrol": [
                        {
                            "#tail": "at the top of the page. Use",
                            "#text": "Results List"
                        },
                        {
                            "#tail": "and",
                            "#text": "Previous Document"
                        },
                        {
                            "#tail": "to display the previous or next document in your results. Use the",
                            "#text": "Next Document"
                        },
                        {
                            "#tail": "button (",
                            "#text": "Reading View"
                        },
                        {
                            "#tail": "button again.",
                            "#text": "Reading View"
                        }
                    ],
                    "image": {
                        "@alt": "Full Doc View",
                        "@href": "docreadingview.png",
                        "#tail": ") to view just the document. (In this view, only a few tools will  be available for working with the document.) To access the full range of tools again, click the"
                    },
                    "#text": "To return to your results list, select"
                },
                "Additionally, you can search the document for either your search terms, or any other terms you specify."
            ]
        },
