        "refbody": {
            "section": {
                "p": {
                    "uicontrol": {
                        "#tail": ".",
                        "#text": "Save"
                    },
                    "#text": "You can move or copy a folder to another folder.  This makes it a subfolder of the one you're moving or copying to. When you are finished, select"
                }
            },
            "properties": {
                "property": [
                    {
                        "propvalue": "Folder",
                        "propdesc": "The name of the folder you are moving."
                    },
                    {
                        "propvalue": "Move folder to",
                        "propdesc": {
                            "uicontrol": {
                                "#tail": ".",
                                "#text": "Create New Folder"
                            },
                            "#text": "Select a destination folder to save the folder you're moving (or copying). To create a new folder and use it as the destination folder, select"
                        }
                    },
                    {
                        "@audience": "WFS",
                        "propvalue": "Manage Users",
                        "propdesc": {
                            "note": {
                                "uicontrol": {
                                    "#tail": "page.",
                                    "#text": "Manage Sharing"
                                },
                                "#text": "The folder you are copying or moving \"inherits\" the permissions of the folder you're moving or copying it to.  If you want to change the permissions for any of the users, use the"
                            },
                            "#text": "This is the list of users you are currently sharing the folder with."
                        }
                    },
                    {
                        "@audience": "WFS",
                        "propvalue": "Editor",
                        "propdesc": "Indicates the user may view items in the folder, and add or modify notes for the items."
                    },
                    {
                        "@audience": "WFS",
                        "propvalue": "Viewer",
                        "propdesc": "Indicates the user may only view items in the folder."
                    }
                ]
            }
        }
    }
