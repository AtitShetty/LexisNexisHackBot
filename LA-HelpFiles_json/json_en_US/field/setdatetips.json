{
    "reference": {
        "@otherprops": "urlapi",
        "@audience": "res;urlapi",
        "@id": "setdatetips",
        "title": "Setting a Starting or Ending Date",
        "prolog": {
            "author": "Barb Bruns",
            "copyright": {
                "copyryear": {
                    "@year": "2009"
                },
                "copyrholder": "LexisNexis"
            },
            "critdates": {
                "created": {
                    "@date": "2012-10-18"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance\u2122 URL API",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "refbody": {
            "section": {
                "note": {
                    "@conref": "urlapi_group_ref.xml#urlapi_group_ref/urlapinote"
                },
                "p": [
                    {
                        "uicontrol": [
                            {
                                "#tail": "parameter of 01/01/2000 to indicate that you want to search content starting with January 1, 2000.  Use the",
                                "#text": "startdate"
                            },
                            {
                                "#tail": "parameter to indicate that you only want to search content that is older than the date you specify.",
                                "#text": "enddate"
                            }
                        ],
                        "#text": "Use this URL to restrict your search to a specific starting or ending date.  For example, use a"
                    },
                    {
                        "b": "URL Template"
                    },
                    {
                        "b": "{startdate|enddate}",
                        "#text": "http://advance.lexis.com/api/search?{search-terms}&{collection}&"
                    }
                ],
                "example": {
                    "b": "startdate=1985-10-25",
                    "#text": "http://advance.lexis.com/api/search?q=burden%20of%20proof&collection=cases&"
                }
            },
            "properties": {
                "prophead": {
                    "propvaluehd": "Value",
                    "propdeschd": "Description"
                },
                "property": [
                    {
                        "propvalue": "search",
                        "propdesc": "Indicates the user wants to run a search and view the results."
                    },
                    {
                        "propvalue": "{search-terms}",
                        "propdesc": {
                            "p": {
                                "userinput": {
                                    "#tail": "in the URL to replace spaces between words in the search string.",
                                    "#text": "%20"
                                },
                                "#text": "Query parameter that contains the search syntax. Use"
                            },
                            "example": {
                                "b": {
                                    "#tail": "=breach%20of%20contract",
                                    "#text": "q"
                                }
                            }
                        }
                    },
                    {
                        "propvalue": "{collection}",
                        "propdesc": {
                            "p": [
                                "Query parameter that specifies the content type (Optional).",
                                {
                                    "xref": {
                                        "@scope": "peer",
                                        "@href": "collectiontips.xml",
                                        "#tail": "."
                                    },
                                    "#text": "For a list of values that can be used with the collection parameter, see"
                                }
                            ],
                            "example": {
                                "b": {
                                    "#tail": "=cases",
                                    "#text": "collection"
                                }
                            }
                        }
                    },
                    {
                        "propvalue": "{startdate}",
                        "propdesc": {
                            "p": "Query parameter that specifies the starting date for the content to be searched (Optional). Only content starting with this date will be searched.",
                            "example": {
                                "b": {
                                    "#tail": "=1985-10-25",
                                    "#text": "startdate"
                                }
                            }
                        }
                    },
                    {
                        "propvalue": "{enddate}",
                        "propdesc": {
                            "p": [
                                "Query parameter that specifies the ending date for the content to be searched (Optional). Only content for this date and older will be searched.",
                                "Supported date formats:"
                            ],
                            "example": {
                                "b": {
                                    "#tail": "=1985-10-25",
                                    "#text": "startdate"
                                }
                            },
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "startdate=1985-10-25",
                                        "dd": "Full date"
                                    },
                                    {
                                        "dt": "startdate=1985",
                                        "dd": "Year"
                                    },
                                    {
                                        "dt": "startdate=5y",
                                        "dd": "Past number of years"
                                    },
                                    {
                                        "dt": "startdate=6m",
                                        "dd": "Past number of months"
                                    },
                                    {
                                        "dt": "startdate=30d",
                                        "dd": "Past number of days"
                                    },
                                    {
                                        "dt": "enddate=1985-10-25",
                                        "dd": "Full date"
                                    },
                                    {
                                        "dt": "enddate=1985",
                                        "dd": "Year"
                                    },
                                    {
                                        "dt": "enddate=5y",
                                        "dd": "Past number of years"
                                    },
                                    {
                                        "dt": "enddate=6m",
                                        "dd": "Past number of months"
                                    },
                                    {
                                        "dt": "enddate=30d",
                                        "dd": "Past number of days"
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        }
    }
}