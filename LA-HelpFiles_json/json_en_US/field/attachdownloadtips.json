{
    "reference": {
        "@otherprops": "attachdownloadtips",
        "@id": "attachdownloadtips",
        "title": "Download Page",
        "prolog": {
            "author": [
                "Barb Bruns",
                "Christina Stommel",
                "Garth Lewis"
            ],
            "copyright": {
                "copyryear": {
                    "@year": "2007"
                },
                "copyrholder": "Lexisnexis"
            },
            "critdates": {
                "created": {
                    "@date": "2006-10-25"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "refbody": {
            "section": {
                "p": {
                    "uicontrol": {
                        "#tail": "dialog box. Details about each item on the dialog box are provided below.",
                        "#text": "Download"
                    },
                    "#text": "You can download either your results list or an individual document. To do this, provide the information for each tab of the"
                },
                "note": {
                    "@type": "note",
                    "ul": {
                        "li": [
                            "Different options may be available depending on what type of document you are downloading.",
                            {
                                "uicontrol": [
                                    {
                                        "#tail": "drop-down list in the global navigation bar and view the",
                                        "#text": "History"
                                    },
                                    {
                                        "#tail": ".  Your document download is shown in the list of your activities (indicated using the type",
                                        "#text": "Research Map"
                                    },
                                    {
                                        "#tail": "). Click this",
                                        "#text": "Download"
                                    },
                                    {
                                        "#tail": "link and choose",
                                        "#text": "Download"
                                    },
                                    {
                                        "#tail": "from the",
                                        "#text": "Retrieve Items"
                                    },
                                    {
                                        "#tail": "list to view your options for retrieving it.",
                                        "#text": "Actions"
                                    }
                                ],
                                "#text": "To view a list of documents you have already downloaded, select the"
                            }
                        ]
                    }
                }
            },
            "properties": {
                "property": [
                    {
                        "@id": "title",
                        "propvalue": {
                            "i": {
                                "#tail": ") tab",
                                "#text": "n"
                            },
                            "#text": "Selected Documents ("
                        },
                        "propdesc": {
                            "uicontrol": {
                                "#tail": ".",
                                "#text": "Select: None"
                            },
                            "#text": "The document titles of the documents you selected to download. If you want to remove any documents before downloading them, you can either clear individual documents, or click"
                        }
                    },
                    {
                        "propvalue": "Basic Options tab",
                        "propdesc": {
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "What do you want to download?",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to download all the documents you selected.",
                                                            "#text": "Full documents (n)"
                                                        },
                                                        "note": {
                                                            "@type": "note",
                                                            "uicontrol": {
                                                                "#tail": "to also include any attached documents associated with the items you are downloading. See",
                                                                "#text": "Include document attachments, where available"
                                                            },
                                                            "xref": {
                                                                "@scope": "peer",
                                                                "@href": "deliverattachments_hdi.xml",
                                                                "#tail": "for details."
                                                            },
                                                            "#text": "Select"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- If any documents you want to download contain attachments, you can choose to download only those attachments (and not the related documents).",
                                                            "#text": "Full document attachments only (where available)"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to download the results list (not the documents themselves), and then enter the specific results you want delivered. You can download up to 250 of the top 1,000 results.",
                                                            "#text": "Results list for <Content Type>"
                                                        },
                                                        "note": [
                                                            {
                                                                "@type": "note",
                                                                "#text": "Public Records delivers up to 200 consecutive as well as non-consecutive items; not 250."
                                                            },
                                                            {
                                                                "@type": "note",
                                                                "uicontrol": {
                                                                    "#tail": "to include a graph that shows the location and distribution of each of your search terms in each of your results.",
                                                                    "#text": "Include the graphical view of search terms (PDF only)"
                                                                },
                                                                "#text": "Select"
                                                            }
                                                        ]
                                                    },
                                                    {
                                                        "uicontrol": [
                                                            {
                                                                "#tail": "- Select this, followed by either",
                                                                "#text": "Litigation Profile Suite report for < Name>"
                                                            },
                                                            {
                                                                "#tail": "to download both the items from the results list you specify as well as the charts and graphs associated with the profile, or",
                                                                "#text": "All charts and graphs with results list"
                                                            },
                                                            {
                                                                "#tail": "to download just the charts and graphs. If you choose to deliver the results list, you can select any number or combination of results up to 250 total. (",
                                                                "#text": "Charts and graphs only"
                                                            }
                                                        ],
                                                        "ph": {
                                                            "@keyref": "lpsname",
                                                            "#tail": "users only.)"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": [
                                                            {
                                                                "#tail": "- Select this, followed by either",
                                                                "#text": "Verdict & Settlement Analyzer report for < search term>"
                                                            },
                                                            {
                                                                "#tail": "to download both the items from the results list you specify as well as the charts and graphs associated with the search, or",
                                                                "#text": "All charts and graphs with results list"
                                                            },
                                                            {
                                                                "#tail": "to download just the charts and graphs. If you choose to deliver the results list, you can select any number or combination of results up to 250 total. (",
                                                                "#text": "Charts and graphs only"
                                                            }
                                                        ],
                                                        "ph": {
                                                            "@keyref": "vsaname",
                                                            "#tail": "users only.)"
                                                        }
                                                    }
                                                ]
                                            },
                                            "#text": "Select one of the following options, depending on whether you're downloading a results list or documents:"
                                        }
                                    },
                                    {
                                        "dt": "Document type",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Downloads the documents in .pdf document. Files downloaded in .pdf format can be opened and viewed using a PDF viewer, such as Adobe\u00ae Acrobat Reader. (Acrobat Reader is an application that may be downloaded free if you do not already have it.)",
                                                            "#text": "Portable Document Format (.PDF)"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Downloads the documents in Microsoft Word 2007 format.",
                                                            "#text": "Microsoft Word-compatible (.DOCX)"
                                                        },
                                                        "fn": "Once the document is saved to disk and you are viewing it in Microsoft\u00ae Word, you can view any of the LexisNexis legal links in the footer by first double-clicking on the footer. Once you do this, the footer (along with the links in it) become active. From here, to open the corresponding web page, press Ctrl as you select the specific link you want to view."
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Downloads the document in WordPerfect-compatible format.",
                                                            "#text": "WordPerfect-compatible (.WPD)"
                                                        }
                                                    },
                                                    {
                                                        "b": {
                                                            "ph": {
                                                                "@keyref": "companyfirst",
                                                                "#tail": "CaseMap"
                                                            },
                                                            "#tail": "- Downloads the documents in"
                                                        },
                                                        "ph": {
                                                            "@keyref": "companyfirst",
                                                            "#tail": "CaseMap format."
                                                        },
                                                        "note": {
                                                            "@type": "note",
                                                            "#text": "If you are downloading documents in CaseMap format, any attachments associated with the documents will be ignored, even if you select to include attachments with the download."
                                                        }
                                                    }
                                                ]
                                            },
                                            "#text": "Select the format in which you would like the documents to be downloaded."
                                        }
                                    },
                                    {
                                        "dt": "When downloading multiple documents",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to format all the documents you selected into a single file.",
                                                            "#text": "Group and save documents as a single file"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to format each document you selected in separate files.",
                                                            "#text": "Save as individual files"
                                                        }
                                                    }
                                                ]
                                            },
                                            "#text": "Select one of the following options:"
                                        }
                                    },
                                    {
                                        "dt": "Compress files in .ZIP format",
                                        "dd": {
                                            "note": {
                                                "@type": "tip",
                                                "#text": "Compressed files take up less space on your computer's local drive. Files saved in ZIP format can be opened and viewed using any file compression application. You can search the Internet for a list of programs (both paid and free) you can use to do this."
                                            },
                                            "#text": "Select this checkbox if you want to download the files in a compressed (ZIP) file format."
                                        }
                                    },
                                    {
                                        "dt": "Filename",
                                        "dd": {
                                            "p": "Enter a filename for the downloaded file."
                                        }
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "propvalue": "Formatting Options tab",
                        "propdesc": {
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "Include the following in the delivered document:",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want an introductory cover page as the first page in your document.",
                                                            "#text": "Cover page"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want",
                                                            "#text": "LexisNexis\u00ae logo"
                                                        },
                                                        "ph": {
                                                            "@keyref": "companyfirst",
                                                            "#tail": "branding in your downloaded document."
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include both your first name and last name on the cover page and in the footer of your documents.",
                                                            "#text": "First and last name on cover page and within footer"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select a position for the page number from the related options.",
                                                            "#text": "Page numbering in document"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": [
                                                            {
                                                                "#tail": "- Select to embed citations and other references within the document and hyperlink them. (Select",
                                                                "#text": "Embedded references as links"
                                                            },
                                                            {
                                                                "#tail": "if you want the embedded legal cites to appear in italic type in your documents).",
                                                                "#text": "Italicize embedded legal citations"
                                                            }
                                                        ]
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select to include document markup from the user, and then specify what type of markup you want included: edits from all users (including your own) or just edits made by others (and not you).",
                                                            "#text": "Users' annotations, highlights, and notes"
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    },
                                    {
                                        "dt": "Font type",
                                        "dd": {
                                            "note": {
                                                "@type": "tip",
                                                "uicontrol": [
                                                    {
                                                        "#tail": "font is best for cases, statutory materials, and any long documents that are typically read in printed form.",
                                                        "#text": "Times New Roman"
                                                    },
                                                    {
                                                        "#tail": "is best for tables and other types of content that don't display well with varying font widths.",
                                                        "#text": "Courier"
                                                    }
                                                ],
                                                "#text": "The"
                                            },
                                            "#text": "Select the font to use in your documents."
                                        }
                                    },
                                    {
                                        "dt": "Font size",
                                        "dd": "Select the desired font size for your documents."
                                    },
                                    {
                                        "dt": "Line spacing",
                                        "dd": {
                                            "note": {
                                                "@type": "note",
                                                "ul": {
                                                    "li": [
                                                        "Double-spacing is not recommended for documents using dual column format.",
                                                        "Double-spacing is not applied to tables, case citations, or spreadsheets."
                                                    ]
                                                }
                                            },
                                            "#text": "Choose either single or double spacing for your documents."
                                        }
                                    },
                                    {
                                        "dt": "Styling",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want pagination to appear in bold type in your documents.",
                                                            "#text": "Bold search terms"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want your search terms to appear in italic type in your documents.",
                                                            "#text": "Italicize search terms"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want your search terms underlined.",
                                                            "#text": "Underline search terms"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want page numbers from the associated reporter to appear in bold type.",
                                                            "#text": "Bold reporter page numbers"
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "propvalue": "Content-Specific Options tab",
                        "propdesc": {
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "Statutes",
                                        "dd": {
                                            "uicontrol": {
                                                "#tail": "to include any annotations that are associated with the statutes.",
                                                "#text": "Include annotations"
                                            },
                                            "#text": "Select"
                                        }
                                    },
                                    {
                                        "dt": "Deliver full case document",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include the Shepard's Signal indicator symbol next to cites in the document. These indicators help you understand whether the cite references \"good law\".",
                                                            "#text": "Show Shepard's Signal\u0099 indicators"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include the headnotes for the case with each document.",
                                                            "#text": "Show Headnotes"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include the core terms for the case with each document.",
                                                            "#text": "Show Core Terms"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include the case summary with each document.",
                                                            "#text": "Show Case Summary"
                                                        }
                                                    }
                                                ]
                                            },
                                            "#text": "Select this option to deliver the full case document, along with the following options:"
                                        }
                                    },
                                    {
                                        "dt": "Deliver Case Summary only",
                                        "dd": {
                                            "note": {
                                                "@type": "note",
                                                "#text": "If no case summary for a delivered document exists, the document will be delivered instead."
                                            },
                                            "#text": "Choose this option to deliver only the case summary."
                                        }
                                    },
                                    {
                                        "dt": "Number of Columns",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to save case law documents in dual column format.",
                                                            "#text": "Dual"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to save documents formatted in a single column.",
                                                            "#text": "Single"
                                                        }
                                                    }
                                                ]
                                            },
                                            "#text": "Choose one of the following:"
                                        }
                                    },
                                    {
                                        "dt": "Secondary Materials",
                                        "dd": [
                                            {
                                                "uicontrol": {
                                                    "#tail": "- For secondary materials that are annotated, select this to include the annotations.",
                                                    "#text": "Include Annotations"
                                                }
                                            },
                                            {
                                                "uicontrol": {
                                                    "#tail": "- Some practice guides include instructional videos. Select this option to include the written transcripts for these videos in your delivery.",
                                                    "#text": "Include video transcripts"
                                                }
                                            }
                                        ]
                                    },
                                    {
                                        "dt": "Statutes",
                                        "dd": {
                                            "uicontrol": {
                                                "#tail": "- For statutes that are annotated, select this to include the annotations.",
                                                "#text": "Include published annotations"
                                            }
                                        }
                                    },
                                    {
                                        "dt": "Include classification information, like language, publication type, and subject keywords",
                                        "dd": "Select this option to include metadata about the news article and how it was categorized."
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "@id": "OK",
                        "propvalue": "Download",
                        "propdesc": "Click this to download your documents."
                    },
                    {
                        "@id": "cancel",
                        "propvalue": "Cancel",
                        "propdesc": "Click this to return to your search results without downloading your documents."
                    }
                ]
            }
        }
    }
}