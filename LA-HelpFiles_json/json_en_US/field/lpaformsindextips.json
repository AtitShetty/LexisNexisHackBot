                        ],
                        "ph": {
                            "@keyref": "lpaname",
                            "#tail": "home page. Click the link for the selected practice area to open the index of forms for that practice area or select"
                        },
                        "#text": "There is a"
                    },
                    {
                        "uicontrol": {
                            "#tail": "fly-out menu.",
                            "#text": "Topics"
                        },
                        "#text": "You can also open the forms index for a particular practice area from the topics menu on any subtopic page within that practice area by selecting the link at the bottom of the"
                    }
                ],
                "simpletable": {
                    "sthead": {
                        "stentry": [
                            "Feature",
                            "Description"
                        ]
                    },
                    "strow": [
                        {
                            "stentry": [
                                {
                                    "image": {
                                        "@alt": "Actions",
                                        "@href": "lpaactions.png",
                                        "@placement": "inline"
                                    }
                                },
                                {
                                    "uicontrol": {
