{
    "reference": {
        "@otherprops": "searchwithtermstips",
        "@audience": "res;urlapi",
        "@id": "searchwithtermstips",
        "title": "Using Search Terms",
        "prolog": {
            "author": "Barb Bruns",
            "copyright": {
                "copyryear": {
                    "@year": "2009"
                },
                "copyrholder": "LexisNexis"
            },
            "critdates": {
                "created": {
                    "@date": "2012-10-18"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance\u2122 URL API",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "refbody": {
            "section": {
                "note": {
                    "@conref": "urlapi_group_ref.xml#urlapi_group_ref/urlapinote"
                },
                "p": [
                    "Use this URL to search using specific search terms.",
                    {
                        "b": "URL Template"
                    },
                    {
                        "b": {
                            "#tail": "&{collection}&{qlang}",
                            "#text": "search?{search-terms}"
                        },
                        "#text": "http://advance.lexis.com/api/"
                    }
                ],
                "example": {
                    "b": {
                        "#tail": "&collection=cases&qlang=bool",
                        "#text": "search?q=burden%20of%20proof"
                    },
                    "#text": "http://advance.lexis.com/api/"
                }
            },
            "properties": {
                "prophead": {
                    "propvaluehd": "Value",
                    "propdeschd": "Description"
                },
                "property": [
                    {
                        "propvalue": "search",
                        "propdesc": "Indicates the user wants to run a search and view the results."
                    },
                    {
                        "propvalue": "{search-terms}",
                        "propdesc": {
                            "p": {
                                "userinput": {
                                    "#tail": "in the URL to replace spaces between words in the search string.",
                                    "#text": "%20"
                                },
                                "#text": "Query parameter that contains the search syntax. Use"
                            },
                            "example": {
                                "b": {
                                    "#tail": "=breach%20of%20contract",
                                    "#text": "q"
                                }
                            }
                        }
                    },
                    {
                        "propvalue": "{collection}",
                        "propdesc": {
                            "p": [
                                "Query parameter that specifies the content type (Optional).",
                                {
                                    "xref": {
                                        "@scope": "peer",
                                        "@href": "collectiontips.xml",
                                        "#tail": "."
                                    },
                                    "#text": "For a list of values that can be used with the collection parameter, see"
                                }
                            ],
                            "example": {
                                "b": {
                                    "#tail": "=cases",
                                    "#text": "collection"
                                }
                            }
                        }
                    },
                    {
                        "propvalue": "{qlang}",
                        "propdesc": {
                            "p": [
                                "Query parameter that specifies the query language (Optional).",
                                "Possible values:"
                            ],
                            "example": {
                                "b": {
                                    "#tail": "=bool",
                                    "#text": "qlang"
                                }
                            },
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "bool",
                                        "dd": "Boolean language"
                                    },
                                    {
                                        "dt": "nat",
                                        "dd": "natural language"
                                    },
                                    {
                                        "dt": "dyn",
                                        "dd": "dynamic (allows the application to determine the query language)"
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        }
    }
}