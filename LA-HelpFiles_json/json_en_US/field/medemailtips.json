{
    "reference": {
        "@otherprops": "medemailtips",
        "@audience": "obsolete",
        "@id": "medemailtips",
        "title": "Email Page",
        "prolog": {
            "author": "Robin Stubbart",
            "copyright": {
                "copyryear": {
                    "@year": "2012"
                },
                "copyrholder": "Lexisnexis"
            },
            "critdates": {
                "created": {
                    "@date": "2012-10-08"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            },
            "data": "medmal navigator medical malpractice litigator litigation"
        },
        "refbody": {
            "section": [
                {
                    "title": "Email Options"
                },
                {
                    "title": "Format Options"
                },
                {
                    "title": "Buttons"
                }
            ],
            "properties": [
                {
                    "property": [
                        {
                            "propvalue": "What do you want to Email?",
                            "propdesc": "Lists the topic you selected for your dashboard."
                        },
                        {
                            "propvalue": "To",
                            "propdesc": "Enter up to 5 recipients' email addresses, separated by a semicolon (;)."
                        },
                        {
                            "propvalue": "Consider including:",
                            "propdesc": "Select from the suggested addresses to include other recipients."
                        },
                        {
                            "propvalue": "Subject",
                            "propdesc": "Enter the text you want to appear on the subject line of the email."
                        },
                        {
                            "propvalue": "Notes",
                            "propdesc": "Enter any notes you want to appear in the body of the email, above the document text."
                        },
                        {
                            "propvalue": "Document Type",
                            "propdesc": {
                                "ul": {
                                    "li": [
                                        "PDF - Emails the documents in .pdf document. Files downloaded in .pdf format can be opened and viewed using the Adobe\u00ae Acrobat Reader, an application that may be downloaded free if you do not already have it.",
                                        "Docx - Emails the documents in Microsoft Word 2007 format."
                                    ]
                                },
                                "#text": "Select the format in which you would like the documents to be emailed."
                            }
                        },
                        {
                            "propvalue": "Send As",
                            "propdesc": {
                                "ul": {
                                    "li": [
                                        "One File - Select this to format all the documents you selected in one file.",
                                        "Separate Files - Select this to format each document you selected in separate files."
                                    ]
                                },
                                "#text": "Select one of the following options from the drop-down list:"
                            }
                        },
                        {
                            "propvalue": "Zip files",
                            "propdesc": {
                                "ul": {
                                    "li": [
                                        "Some email servers are unable to accept large file sizes. Smaller, compressed files are more likely to be received.",
                                        "Compressed files take up less space in the recipient's mailbox.",
                                        "When downloading, compressed files take up less space on your computer's local drive."
                                    ]
                                },
                                "note": {
                                    "@type": "note",
                                    "#text": "Files sent in ZIP format can be opened and viewed using WinZip\u00ae, an application that may be purchased from this location: http://www.winzip.com/downwz.htm if the recipient does not already have it."
                                },
                                "#text": "Select this checkbox if you want to send the files in a compressed (ZIP) file format."
                            }
                        }
                    ]
                },
                {
                    "property": [
                        {
                            "propvalue": {
                                "ph": {
                                    "@keyref": "companyfirst",
                                    "#tail": "logos"
                                },
                                "#text": "Include"
                            },
                            "propdesc": {
                                "ph": {
                                    "@keyref": "companyfirst",
                                    "#tail": "branding."
                                },
                                "#text": "Select this if you want to email your documents with"
                            }
                        },
                        {
                            "propvalue": "Display first and last name",
                            "propdesc": "Select this to include both your first name and last name on the cover page and in the footer of your documents."
                        },
                        {
                            "propvalue": "Include page number",
                            "propdesc": "Select a position for the page number."
                        }
                    ]
                },
                {
                    "property": [
                        {
                            "@id": "OK",
                            "propvalue": "Email",
                            "propdesc": "Select this to email your documents."
                        },
                        {
                            "@id": "cancel",
                            "propvalue": "Cancel",
                            "propdesc": "Select this to return to your search results without saving your documents."
                        }
                    ]
                }
            ]
        }
    }
}