{
    "reference": {
        "@otherprops": "urlapi",
        "@audience": "res;urlapi",
        "@id": "displayTOCtips",
        "title": "Display a Table of Contents for a Specific Source",
        "prolog": {
            "author": "Barb Bruns",
            "copyright": {
                "copyryear": {
                    "@year": "2009"
                },
                "copyrholder": "LexisNexis"
            },
            "critdates": {
                "created": {
                    "@date": "2012-10-18"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance\u2122 URL API",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "refbody": {
            "section": {
                "note": {
                    "@conref": "urlapi_group_ref.xml#urlapi_group_ref/urlapinote"
                },
                "p": [
                    {
                        "ph": {
                            "@keyref": "companyfirst",
                            "#tail": "account representative to find out the value for the source for which you want the TOC."
                        },
                        "#text": "Use this URL to display a table of contents (TOC) for a specific source. Contact your your"
                    },
                    "Not every source has a table of contents. If you specify a source that doesn't have a TOC, you'll see an error message.",
                    {
                        "b": "URL Template"
                    },
                    {
                        "b": "toc/source/{source-id}",
                        "#text": "http://advance.lexis.com/api/"
                    },
                    {
                        "b": {
                            "#tail": "Please contact your account representative for a valid source value.",
                            "#text": "*"
                        }
                    },
                    null
                ],
                "example": {
                    "b": "toc/source/source-id*",
                    "#text": "http://advance.lexis.com/api/"
                }
            },
            "properties": {
                "prophead": {
                    "propvaluehd": "Value",
                    "propdeschd": "Description"
                },
                "property": [
                    {
                        "propvalue": "toc",
                        "propdesc": "Directs to the table of contents."
                    },
                    {
                        "propvalue": "source",
                        "propdesc": "Indicates that the user wants to display the table of contents for a source."
                    },
                    {
                        "propvalue": "{source-id}",
                        "propdesc": {
                            "p": {
                                "ph": {
                                    "@keyref": "company",
                                    "#tail": "account representative to find out the value for the source for which you want the TOC."
                                },
                                "#text": "The identifier for a specific source. Contact your your"
                            },
                            "example": {
                                "b": {
                                    "#tail": "=source-id",
                                    "#text": "source"
                                }
                            }
                        }
                    }
                ]
            }
        }
    }
}