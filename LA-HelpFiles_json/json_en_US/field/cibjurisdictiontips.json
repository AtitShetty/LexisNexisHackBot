        "refbody": {
            "section": [
                {
                    "p": [
                        {
                            "ph": {
                                "@keyref": "prodnamefirst",
                                "#tail": "Jurisdiction page to select the sources you want to search. You can search for a source or just browse through the list to find the one you want to use."
                            },
                            "#text": "Use the"
                        },
                        {
                            "uicontrol": {
                                "#tail": "to select it. A search box appears, showing the source you selected. When you have finished selecting sources, select a date restriction if available, enter your search terms and select the search icon (",
                                "#text": "Add source as a search filter"
                            },
                            "image": {
                                "@alt": "Search all",
                                "@href": "search.png",
                                "@placement": "inline",
                                "#tail": ")."
                            },
                            "#text": "Select a source name, then select"
                        },
                        {
                            "ph": {
                                "@keyref": "prodnamefirst",
                                "#tail": "Jurisdiction page your firm is using may or may not have all of the features described below."
                            },
                            "#text": "The"
                        }
                    ],
                    "note": "Source names displayed in orange are not within your subscription.  There is no charge to search in these sources, but there may be a charge for transactional documents you choose to view."
                },
                {
                    "title": "Listed by Jurisdiction"
                }
            ],
            "properties": [
                {
                    "property": {
                        "propvalue": "Find a Source",
                        "propdesc": {
                            "uicontrol": {
                                "#tail": "to locate the source.",
                                "#text": "Find Source"
                            },
                            "#text": "Enter the name or partial name of a source you want to use, and select"
                        }
                    }
                },
                {
                    "property": [
                        {
                            "propvalue": {
                                "image": {
                                    "@alt": "Search all",
                                    "@href": "search.png",
                                    "@placement": "inline"
                                },
                                "b": "+"
                            },
                            "propdesc": "Click this to display all the sources for a specific jurisdiction."
                        },
                        {
                            "propvalue": "Add source as a search filter",
                            "propdesc": "Click this link under a source name to add it to your search."
                        },
                        {
                            "propvalue": {
                                "image": {
                                    "@alt": "Source Information",
                                    "@href": "info.png",
                                    "@placement": "inline"
                                }
                            },
                            "propdesc": "Click this icon next to a source name to view information about the source."
                        },
                        {
                            "propvalue": "Table of Contents",
                            "propdesc": "Click this link under a source name to view the table of contents for the source."
                        },
                        {
                            "propvalue": "Date Restriction",
                            "propdesc": "Select a date restriction (if available on the search box) if desired."
                        },
                        {
                            "propvalue": {
                                "image": {
                                    "@alt": "Source Information",
                                    "@href": "search.png",
                                    "@placement": "inline"
                                }
                            },
                            "propdesc": "Click this on the search box to start your search."
                        },
                        {
                            "propvalue": "Clear all",
                            "propdesc": "Click this to clear all your selections."
                        }
                    ]
                }
            ]
        }
