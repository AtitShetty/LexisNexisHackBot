{
    "reference": {
        "@otherprops": "cib",
        "@id": "cibgetandprintips",
        "title": "Get & Print Page",
        "prolog": {
            "author": "Barb Bruns",
            "copyright": {
                "copyryear": {
                    "@year": "2007"
                },
                "copyrholder": "LexisNexis"
            },
            "critdates": {
                "created": {
                    "@date": "2009-12-29"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "refbody": {
            "section": {
                "p": {
                    "ph": {
                        "@keyref": "shepardsfirst",
                        "#tail": "reports."
                    },
                    "#text": "Use the Get & Print tool to email, print, or download legal documents or"
                }
            },
            "properties": {
                "property": [
                    {
                        "propvalue": {
                            "ph": {
                                "@keyref": "shepards",
                                "#tail": "reports."
                            },
                            "#text": "Enter a citation or list of citations separated by semicolons (;) or on separate lines. You can enter up to 100 citations to retrieve documents only, or up to 25 citations if you include"
                        },
                        "propdesc": {
                            "p": [
                                {
                                    "userinput": "480 us 102;812 f.2d 911;69 n.j 123",
                                    "#text": "For example, enter"
                                },
                                {
                                    "ph": {
                                        "@keyref": "shepardsfirst",
                                        "#tail": "citations formats, see"
                                    },
                                    "xref": {
                                        "@scope": "external",
                                        "@href": "https://web.lexis.com/help/research/shepardscitationformats_ref.htm",
                                        "#text": "Shepard's(R) Citation Format List"
                                    },
                                    "#text": "For a list of all"
                                }
                            ]
                        }
                    },
                    {
                        "propvalue": "Clear",
                        "propdesc": "Select this to remove any citations you entered."
                    },
                    {
                        "@id": "printoptions",
                        "propvalue": "Basic Options",
                        "propdesc": {
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "Retrieve document",
                                        "dd": "Check this box to retrieve the case law document for each citation you entered."
                                    },
                                    {
                                        "dt": {
                                            "ph": {
                                                "@keyref": "shepards",
                                                "#tail": "Report"
                                            },
                                            "#text": "Retrieve"
                                        },
                                        "dd": {
                                            "ph": {
                                                "@keyref": "shepards",
                                                "#tail": "report for each citation you entered."
                                            },
                                            "#text": "Check this box to retrieve the"
                                        }
                                    },
                                    {
                                        "dt": "Categories",
                                        "dd": {
                                            "ph": {
                                                "@keyref": "shepards",
                                                "#tail": "report."
                                            },
                                            "#text": "Select the categories you want to include on the"
                                        }
                                    },
                                    {
                                        "dt": {
                                            "ph": {
                                                "@keyref": "shepards",
                                                "#tail": "analysis"
                                            },
                                            "#text": "Include"
                                        },
                                        "dd": {
                                            "ph": {
                                                "@keyref": "shepards",
                                                "#tail": "report."
                                            },
                                            "#text": "Select the analyses you want to include on the"
                                        }
                                    },
                                    {
                                        "dt": "Other Citing Sources",
                                        "dd": "Select this to include other citing sources."
                                    },
                                    {
                                        "dt": "Tables of Authorities",
                                        "dd": {
                                            "ph": {
                                                "@keyref": "shepards",
                                                "#tail": "table of authorities with the report."
                                            },
                                            "#text": "Select this to include the"
                                        }
                                    },
                                    {
                                        "dt": "Delivery Type",
                                        "dd": "Select how you want the documents to be delivered (Print, Download, or Email)."
                                    },
                                    {
                                        "dt": "Use attached printer",
                                        "dd": "Select this to print the documents on your attached printer."
                                    },
                                    {
                                        "dt": "Use LexisNexis\u00ae printer",
                                        "dd": "Select this to print the documents using the LexisNexis print utility, which streamlines the print and download request process."
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "propvalue": "Format Options (these options may differ slightly, depending on the type of delivery you select)",
                        "propdesc": {
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "Include these elements",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want an introductory cover page as the first page in your document.",
                                                            "#text": "Cover page"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want",
                                                            "#text": "LexisNexis\u00ae logo"
                                                        },
                                                        "ph": {
                                                            "@keyref": "companyfirst",
                                                            "#tail": "branding in your printed document attachment."
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include both your first name and last name on the cover page and in the footer of your documents.",
                                                            "#text": "First and last name on cover page and within footer"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select a position for the page number from the related options.",
                                                            "#text": "Page numbering in document"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": [
                                                            {
                                                                "#tail": "- Select to embed citations and other references within the document and hyperlink them. (Select",
                                                                "#text": "Embedded references as links"
                                                            },
                                                            {
                                                                "#tail": "if you want the embedded legal cites to appear in italic type in your documents).",
                                                                "#text": "Italicize embedded legal citations"
                                                            }
                                                        ]
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want page numbers from the associated reporter to appear in bold type.",
                                                            "#text": "Display pagination in bold"
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    },
                                    {
                                        "dt": "Font type",
                                        "dd": {
                                            "note": {
                                                "@type": "tip",
                                                "uicontrol": [
                                                    {
                                                        "#tail": "font is best for cases, statutory materials, and any long documents that are typically read in printed form.",
                                                        "#text": "Times New Roman"
                                                    },
                                                    {
                                                        "#tail": "is best for tables and other types of content that don't display well with varying font widths.",
                                                        "#text": "Courier"
                                                    }
                                                ],
                                                "#text": "The"
                                            },
                                            "#text": "Select the font to use in your documents."
                                        }
                                    },
                                    {
                                        "dt": "Font size",
                                        "dd": "Select the desired font size for your documents."
                                    },
                                    {
                                        "dt": "Line spacing",
                                        "dd": {
                                            "note": {
                                                "@type": "note",
                                                "ul": {
                                                    "li": [
                                                        "Double-spacing is not recommended for documents using dual column format.",
                                                        "Double-spacing is not applied to tables, case citations, or spreadsheets."
                                                    ]
                                                }
                                            },
                                            "#text": "Choose either single or double spacing for your documents."
                                        }
                                    },
                                    {
                                        "dt": "Search Terms",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want pagination to appear in bold type in your documents.",
                                                            "#text": "Bold search terms"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want your search terms to appear in italic type in your documents.",
                                                            "#text": "Italicize search terms"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this if you want your search terms underlined.",
                                                            "#text": "Underline search terms"
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "propvalue": "Content-Specific Options",
                        "propdesc": {
                            "dl": {
                                "dlentry": [
                                    {
                                        "dt": "Cases",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "i": {
                                                                "#tail": "Signal\u0099 indicator",
                                                                "#text": "Shepard's"
                                                            },
                                                            "#tail": "- Select this to include the",
                                                            "#text": "Include"
                                                        },
                                                        "i": {
                                                            "#tail": "Signal indicator symbol next to cites in the documents. These indicators help you understand whether the cite references \"good law\".",
                                                            "#text": "Shepard's"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include the headnotes for the case with each document.",
                                                            "#text": "Include headnotes"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include the core terms for the case with each document.",
                                                            "#text": "Include core terms"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Select this to include the case summary with each document.",
                                                            "#text": "Include case summary"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- Choose one of the following:",
                                                            "#text": "Number of Columns"
                                                        },
                                                        "ul": {
                                                            "li": [
                                                                {
                                                                    "uicontrol": {
                                                                        "#tail": "- Select this to save documents formatted in a single column.",
                                                                        "#text": "Single"
                                                                    }
                                                                },
                                                                {
                                                                    "uicontrol": {
                                                                        "#tail": "- Select this to save case law documents in dual column format.",
                                                                        "#text": "Dual"
                                                                    }
                                                                }
                                                            ]
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    },
                                    {
                                        "dt": "Forms",
                                        "dd": {
                                            "ul": {
                                                "li": [
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- For forms that are annotated, select this to include the annotations.",
                                                            "#text": "Include published annotations and comments"
                                                        }
                                                    },
                                                    {
                                                        "uicontrol": {
                                                            "#tail": "- For forms that contain footnote and margin notes, select this to include them.",
                                                            "#text": "Include footnotes and margin notes"
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    },
                                    {
                                        "dt": "Secondary Materials",
                                        "dd": {
                                            "uicontrol": {
                                                "#tail": "- For secondary materials that are annotated, select this to include the annotations.",
                                                "#text": "Include Annotations"
                                            }
                                        }
                                    },
                                    {
                                        "dt": "Statutes",
                                        "dd": {
                                            "uicontrol": {
                                                "#tail": "- For statutes that are annotated, select this to include the annotations.",
                                                "#text": "Include published annotations"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "propvalue": "Get",
                        "propdesc": "Select this to deliver your documents."
                    },
                    {
                        "propvalue": "Status of Request",
                        "propdesc": "The status of your delivery appears here.  If a document cannot be delivered because of insufficient information or some other reason, you may have the opportunity to correct the problem from this box."
                    }
                ]
            }
        }
    }
}