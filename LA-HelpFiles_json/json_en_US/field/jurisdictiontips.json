{
    "reference": {
        "@otherprops": "urlapi",
        "@audience": "res;urlapi",
        "@id": "jurisdictiontips",
        "title": "Search in a Specific Jurisdiction",
        "prolog": {
            "author": "Barb Bruns",
            "copyright": {
                "copyryear": {
                    "@year": "2009"
                },
                "copyrholder": "LexisNexis"
            },
            "critdates": {
                "created": {
                    "@date": "2012-10-18"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance\u00ae URL API",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "refbody": {
            "section": {
                "note": {
                    "@conref": "urlapi_group_ref.xml#urlapi_group_ref/urlapinote"
                },
                "p": [
                    {
                        "uicontrol": {
                            "#tail": "parameter to indicate the jurisdiction to search.  You can use multiple jurisdictions, separated by a period.  If you don't specify a jurisdiction, all jurisdictions are searched.",
                            "#text": "jurisdiction"
                        },
                        "#text": "Use the"
                    },
                    "The jurisdiction parameter is optional. It is shown below in a URL using search terms.",
                    {
                        "xref": {
                            "@scope": "peer",
                            "@href": "jurisdictionvalues_ref.xml",
                            "#tail": ".",
                            "#text": "Values for the Jurisdiction Parameter"
                        },
                        "#text": "For a list of jurisdictions and the values to use for the parameter, see"
                    },
                    {
                        "b": "URL Template"
                    },
                    {
                        "b": "{jurisdiction}",
                        "#text": "http://advance.lexis.com/api/search?{search-terms}&"
                    }
                ],
                "example": {
                    "b": "jurisdiction=dXJuOmpwZjoxMDk5",
                    "#text": "http://advance.lexis.com/api/search?q=burden%20of%20proof&"
                }
            },
            "properties": {
                "prophead": {
                    "propvaluehd": "Value",
                    "propdeschd": "Description"
                },
                "property": [
                    {
                        "propvalue": "search",
                        "propdesc": "Indicates the user wants to run a search and view the results."
                    },
                    {
                        "propvalue": "{search-terms}",
                        "propdesc": {
                            "p": {
                                "userinput": {
                                    "#tail": "in the URL to replace spaces between words in the search string.",
                                    "#text": "%20"
                                },
                                "#text": "Query parameter that contains the search syntax. Use"
                            },
                            "example": {
                                "b": {
                                    "#tail": "=breach%20of%20contract",
                                    "#text": "q"
                                }
                            }
                        }
                    },
                    {
                        "propvalue": "{jurisdiction}",
                        "propdesc": {
                            "p": "Query parameter that specifies the jurisdiction to search in (Optional).",
                            "example": {
                                "b": {
                                    "#tail": "=dXJuOmpwZjoxMDk5",
                                    "#text": "jurisdiction"
                                }
                            }
                        }
                    }
                ]
            }
        }
    }
}