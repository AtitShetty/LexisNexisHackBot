        "title": {
            "ph": {
                "@keyref": "mednamefirst",
                "#tail": "?"
            },
            "#text": "How do I select a topic to use with"
        },
        "prolog": {
            "author": [
                "Robin Stubbart",
                "Garth Lewis"
            ],
            "copyright": {
                "copyryear": {
                    "@year": "2012"
                },
