{
    "task": {
        "@otherprops": "searchadvanced",
        "@audience": "res",
        "@id": "retrievebycitation_hdi",
        "title": "How do I retrieve a document when I know its citation?",
        "prolog": {
            "author": "Barb Bruns",
            "copyright": {
                "copyryear": {
                    "@year": "2012"
                },
                "copyrholder": "Lexisnexis"
            },
            "critdates": {
                "created": {
                    "@date": "2006-10-03"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "taskbody": {
            "context": {
                "p": "To retrieve a single document when you know its citation, follow these steps."
            },
            "steps": {
                "step": [
                    {
                        "cmd": "Enter the document citation in the search box."
                    },
                    {
                        "cmd": {
                            "image": {
                                "@alt": "Search",
                                "@href": "redmagnifyglass.png",
                                "@placement": "inline",
                                "#tail": "."
                            },
                            "#text": "Select"
                        }
                    }
                ]
            }
        },
        "task": {
            "@otherprops": "search",
            "@id": "getandprintabs",
            "title": "To retrieve documents by citation, party name, or docket number:",
            "taskbody": {
                "context": null,
                "steps": {
                    "step": [
                        {
                            "cmd": {
                                "uicontrol": {
                                    "#tail": ".",
                                    "#text": "Get a Doc Assistance"
                                },
                                "#text": "From the home page, select"
                            }
                        },
                        {
                            "cmd": {
                                "uicontrol": [
                                    {
                                        "#tail": ",",
                                        "#text": "By Citation"
                                    },
                                    {
                                        "#tail": ", or",
                                        "#text": "Cases by Party Name"
                                    },
                                    {
                                        "#tail": ".",
                                        "#text": "Cases by Docket Number"
                                    }
                                ],
                                "#text": "Select"
                            }
                        },
                        {
                            "cmd": "Follow the instructions on the screen to complete the form and retrieve the documents you want."
                        }
                    ]
                }
            },
            "task": {
                "@otherprops": "search",
                "@id": "getandprint",
                "title": {
                    "ph": {
                        "@keyref": "shepardsfirst",
                        "#tail": "reports for them, when you know the citations:"
                    },
                    "#text": "To enter multiple citations and download or print the documents or"
                },
                "taskbody": {
                    "steps": {
                        "step": [
                            {
                                "cmd": {
                                    "b": {
                                        "ph": {
                                            "@keyref": "prodnamefirst"
                                        },
                                        "#tail": "at the top of the page."
                                    },
                                    "#text": "Select the down arrow next to"
                                }
                            },
                            {
                                "cmd": {
                                    "uicontrol": {
                                        "#tail": ".",
                                        "#text": "Get & Print"
                                    },
                                    "#text": "Select"
                                }
                            },
                            {
                                "cmd": "Complete the form that appears."
                            },
                            {
                                "cmd": {
                                    "uicontrol": {
                                        "#tail": ".",
                                        "#text": "Go"
                                    },
                                    "#text": "Select"
                                }
                            }
                        ]
                    }
                }
            }
        }
    }
}