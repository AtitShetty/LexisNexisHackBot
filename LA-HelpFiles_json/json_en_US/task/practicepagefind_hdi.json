        "taskbody": {
            "context": {
                "p": "Your firm may use a practice center, such as New York, Banking and Finance, Military Justice, or one of several others. To find your practice center:"
            },
            "steps": {
                "step": [
                    {
                        "cmd": {
                            "ph": {
                                "@keyref": "prodnamefirst",
                                "#tail": "home page, select"
                            },
                            "uicontrol": {
                                "#tail": ".",
                                "#text": "Browse"
                            },
                            "#text": "On the"
                        }
                    },
                    {
                        "cmd": {
                            "uicontrol": {
                                "#tail": ".",
                                "#text": "Practice Center"
                            },
                            "#text": "Select"
                        }
                    },
                    {
                        "cmd": {
                            "uicontrol": [
                                {
                                    "#tail": "or",
                                    "#text": "By Practice Area"
                                },
                                {
                                    "#tail": ".",
                                    "#text": "By Jurisdiction"
                                }
                            ],
                            "#text": "Select"
                        }
                    },
                    {
                        "cmd": "From the list of practice centers, select the one you use.",
                        "info": {
                            "note": {
                                "@type": "note",
                                "ph": {
                                    "@keyref": "prodname",
                                    "#tail": "start page, select the"
                                },
                                "uicontrol": [
                                    {
                                        "#tail": "drop-down menu and select",
                                        "#text": "Actions"
                                    },
                                    {
                                        "#tail": ". To change your start page again, go to",
                                        "#text": "Make this my research start page"
                                    },
                                    {
                                        "#tail": ".",
                                        "#text": "Settings"
                                    }
                                ],
                                "#text": "To set the practice center as your"
                            }
                        }
                    }
                ]
            }
        }
    }
