{
    "task": {
        "@otherprops": "searchbrowse",
        "@id": "archivedcodesearch_hdi",
        "title": "How do I search archived codes?",
        "prolog": {
            "author": "Barb Bruns",
            "copyright": {
                "copyryear": {
                    "@year": "2015"
                },
                "copyrholder": "Lexisnexis"
            },
            "critdates": {
                "created": {
                    "@date": "2006-10-03"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "taskbody": {
            "context": {
                "p": [
                    "You can search a collection of archived codes, including statutory codes, constitutions, administrative codes, municipal codes, and court rules.",
                    "On the home page:"
                ]
            },
            "steps": {
                "step": [
                    {
                        "cmd": {
                            "uicontrol": [
                                {
                                    "#tail": "on the",
                                    "#text": "Archived Codes Search"
                                },
                                {
                                    "#tail": "pod.",
                                    "#text": "Archives"
                                }
                            ],
                            "#text": "Select"
                        }
                    },
                    {
                        "cmd": {
                            "uicontrol": [
                                {
                                    "#tail": "box on the",
                                    "#text": "Enter search terms"
                                },
                                {
                                    "#tail": "page, type the words you want to search for.",
                                    "#text": "Search:Archived codes"
                                }
                            ],
                            "#text": "In the"
                        }
                    },
                    {
                        "cmd": {
                            "uicontrol": {
                                "#tail": ", select the category for the codes you want to search, the year or years to include in the search, and the jurisdiction, if desired.",
                                "#text": "Narrow by"
                            },
                            "#text": "Under"
                        }
                    },
                    {
                        "cmd": {
                            "uicontrol": {
                                "#tail": ".",
                                "#text": "Search"
                            },
                            "#text": "Select"
                        }
                    }
                ]
            }
        }
    }
}