{
    "task": {
        "@otherprops": "delivery;attachdownloadtips;deliveryrangetips",
        "@id": "downloadresults_hdi",
        "title": "How do I download my results or documents?",
        "prolog": {
            "author": [
                "Barb Bruns",
                "Christina Stommel"
            ],
            "copyright": {
                "copyryear": {
                    "@year": "2009"
                },
                "copyrholder": "Lexisnexis"
            },
            "critdates": {
                "created": {
                    "@date": "2009-12-15"
                }
            },
            "metadata": {
                "prodinfo": {
                    "prodname": "Lexis Advance",
                    "vrmlist": {
                        "vrm": {
                            "@version": "1"
                        }
                    }
                }
            }
        },
        "taskbody": {
            "context": {
                "p": [
                    "You can download documents selected from your results list, or download a document you have open.",
                    "To do this:"
                ],
                "note": {
                    "@type": "note",
                    "xref": {
                        "@scope": "peer",
                        "@href": "deliverattachments_hdi.xml",
                        "#tail": "for details on downloading any attachments associated with your delivery."
                    },
                    "#text": "See"
                }
            },
            "steps": {
                "step": [
                    {
                        "cmd": "Choose whether you want to download a single document, or download the results list:",
                        "choicetable": {
                            "chrow": [
                                {
                                    "choption": "To download the results list",
                                    "chdesc": {
                                        "note": [
                                            {
                                                "@type": "tip",
                                                "uicontrol": [
                                                    {
                                                        "#tail": "button located in the upper-right corner of the list, or by clicking the",
                                                        "#text": "Clear All"
                                                    },
                                                    {
                                                        "#tail": "next to the specific item you want to remove.",
                                                        "#text": "X"
                                                    }
                                                ],
                                                "#text": "You can easily clear items from your delivery tray by selecting the delivery count\tdrop-down list and then by either clicking the"
                                            },
                                            {
                                                "@type": "note",
                                                "#text": "Public Records delivers up to 200 consecutive as well as non-consecutive items; not 250."
                                            }
                                        ],
                                        "#text": "While viewing your results list, select the specific results you want to download, or don't select any results to download 250 of the first 1,000 results in your list."
                                    }
                                },
                                {
                                    "choption": "To download a single document",
                                    "chdesc": "While viewing the results list, open the specific document you want to download."
                                }
                            ]
                        }
                    },
                    {
                        "cmd": "Choose whether you want to download your delivery immediately (using your last-used download settings), or select new download settings before completing the request:",
                        "choicetable": {
                            "chrow": [
                                {
                                    "choption": "To download your selection immediately",
                                    "chdesc": {
                                        "uicontrol": {
                                            "#tail": "icon (",
                                            "#text": "Download"
                                        },
                                        "image": {
                                            "@alt": "Download",
                                            "@href": "download.png",
                                            "#tail": ") in the delivery toolbar. The downloading process is started immediately and no further steps are required."
                                        },
                                        "#text": "Click the"
                                    }
                                },
                                {
                                    "choption": "To select new settings before downloading your selection",
                                    "chdesc": {
                                        "uicontrol": {
                                            "#tail": "icon (",
                                            "#text": "Settings"
                                        },
                                        "image": [
                                            {
                                                "@alt": "Delivery Settings",
                                                "@href": "deliverysettings.png",
                                                "#tail": ") next to the Download icon ("
                                            },
                                            {
                                                "@alt": "Download",
                                                "@href": "download.png",
                                                "#tail": ") and provide the required information on the Download dialog box. (See"
                                            }
                                        ],
                                        "xref": {
                                            "@scope": "peer",
                                            "@href": "attachdownloadtips.xml",
                                            "#tail": ".)"
                                        },
                                        "#text": "Click the"
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        }
    }
}