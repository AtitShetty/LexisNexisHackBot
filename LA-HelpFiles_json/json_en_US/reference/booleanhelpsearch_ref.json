        "refbody": {
            "section": {
                "note": {
                    "@type": "note",
                    "ph": {
                        "@keyref": "prodnamefirst",
                        "#tail": "."
                    },
                    "p": {
                        "@audience": "res",
                        "ph": {
                            "@keyref": "prodname",
                            "#tail": ", see"
                        },
                        "xref": {
                            "@scope": "peer",
                            "@href": "useconnectors_hdi.xml",
                            "#tail": "."
                        },
                        "#text": "For information on using connectors to search"
                    },
                    "#text": "This article is related to searching the Online Help System - not"
                },
                "simpletable": {
                    "sthead": {
                        "stentry": [
                            "To find documents containing...",
                            "Enter..."
                        ]
                    },
                    "strow": [
                        {
                            "stentry": [
                                "The phrase \"table of contents\"",
                                {
                                    "userinput": "\"table of contents\""
                                }
                            ]
                        },
                        {
                            "stentry": [
                                "The word \"research\" or the phrase \"table of contents\" or both",
                                {
                                    "p": [
                                        {
                                            "userinput": "research \"table of contents\""
                                        },
                                        "or",
                                        {
                                            "userinput": "research OR \"table of contents\""
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "stentry": [
                                "The word \"research\" and the phrase \"table of contents\"",
                                {
                                    "userinput": "research AND \"table of contents\""
                                }
                            ]
                        },
                        {
                            "stentry": [
                                "The word \"research\" but not the word \"lexisnexis\"",
                                {
                                    "p": [
                                        {
                                            "userinput": "research\u00a0NOT\u00a0lexisnexis"
                                        },
                                        "or",
                                        {
                                            "userinput": "research\u00a0-lexisnexis"
                                        },
                                        "or",
                                        {
                                            "userinput": "research\u00a0!lexisnexis"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        }
    }
